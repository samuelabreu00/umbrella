{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "s-maxage=1, stale-while-revalidate" }
        ]
      }
    ]
  }
  